{
	"compilerOptions": {
		"target": "ES2023",
		"lib": ["ES2023"],
		"module": "CommonJS",
		"moduleResolution": "Node",

		"outDir": "dist",
		"sourceMap": true,
		"composite": true,
		"declaration": true,
		"strict": true,
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}
