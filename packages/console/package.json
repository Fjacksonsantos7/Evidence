		"@evidence-dev/evidence": "workspace:^"
	},
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/package": "^2.0.0",
		"apache-arrow": "^12.0.1",
		"publint": "^0.1.9",
		"svelte": "^3.54.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.5",
		"vite": "^4.0.4"
